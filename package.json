{
    "name": "sainsorsciencebot",
    "version": "0.3.0",
    "description": "A bot whom provides science quizzes from Malaysia's secondary schools with both Malay and English support.",
    "main": "dist/index.js",
    "type": "module",
    "scripts": {
        "cleanMac": "rm -rf dist",
        "cleanWindows": "del /s /q dist",
        "startMac": "npm run cleanMac && npm run build && node dist/index.js",
        "startWindows": "npm run cleanWindows && npm run build && node dist/index.js",
        "startNoClean": "npm run build && node dist/index.js",
        "build": "npx tsc --build",
        "release": "npx standard-version"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/haydenykh/SainsOrScienceBot.git"
    },
    "keywords": [
        "discord",
        "sains",
        "science",
        "bot"
    ],
    "author": "HaydenYKH",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/haydenykh/SainsOrScienceBot/issues"
    },
    "homepage": "https://github.com/haydenykh/SainsOrScienceBot#readme",
    "devDependencies": {
        "@eslint/js": "^9.9.0",
        "@types/node": "^22.4.1",
        "eslint": "^9.9.0",
        "globals": "^15.9.0",
        "standard-version": "^9.5.0",
        "tsx": "^4.17.0",
        "typescript": "^5.5.4",
        "typescript-eslint": "^8.1.0"
    },
    "dependencies": {
        "chalk": "^5.3.0",
        "discord.js": "^14.15.3",
        "dotenv": "^16.4.5",
        "glob": "^11.0.0"
    }
}
